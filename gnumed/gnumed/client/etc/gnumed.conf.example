[workplace]
help desk = one-line contact information here
name = <name for this workplace>

[profile GnuMed - Public DB at hherb.com]
host = hherb.com
port = 5432
database = gnumed

[profile gnumed at localhost]
host = localhost
port = 5432
database = gnumed

[profile GnuMed - Public DB at anubis]
host = anubis.homeunix.com
port = 5432
database = gnumed

[backend]
