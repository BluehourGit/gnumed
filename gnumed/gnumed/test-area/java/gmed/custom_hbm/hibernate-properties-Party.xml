

<set role="telephones" table="party_telephone_link" lazy="true">
	<key column="party_id"/>
	<many-to-many class="xgmed.domain.accountability.Telephone" column="telephone_id"/>
</set>

<set role="partyTypes" table="party_partytype_link" lazy="false">
    <key column="party_id"/>
    <many-to-many class="xgmed.domain.accountability.PartyType" column="partytype_id"/>
</set>

<set role="observations" lazy="true" readonly="true">
	<key column="subject"/>
	<one-to-many class="xgmed.domain.observation.Observation"/>
</set>
	
		
