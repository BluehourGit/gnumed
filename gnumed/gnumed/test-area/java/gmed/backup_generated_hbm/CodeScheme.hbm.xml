<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class 
		name="xgmed.domain.observation.CodeScheme"
		table="codeScheme"
	>

	    <id 
			name="id"
			type="long"
		>
			<generator class="hilo.long">
			</generator>
	    </id>

        <property
	    	name="description"
            type="java.lang.String"
            column="description"
            not-null="false"
			unique="false"
		/>

        <property
	    	name="idExternal"
            type="java.lang.String"
            column="idExternal"
            not-null="false"
			unique="false"
		/>

	    <!--
			To add non XDoclet properties, create a file named
			    hibernate-properties-CodeScheme.xml
			containing the additional properties and place it in your merge dir.
	    -->

	</class>

</hibernate-mapping>
