<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class 
		name="xgmed.domain.planning.resource.Holding"
		table="holding"
	>

	    <id 
			name="id"
			type="long"
		>
			<generator class="hilo.long">
			</generator>
	    </id>

       <many-to-one
	       name="consumableType"
		   class="xgmed.domain.planning.resource.ConsumableType"
		   column="consumableType"
		   not-null="false"
		   unique="false"
		   cascade="none"
		   outer-join="auto"
        />

<set role="consumableAllocations"  readonly="true" lazy="true">
	<key column="holding"/>
	<one-to-many class="xgmed.domain.planning.resource.ConsumableAllocation"/>
</set>

	</class>

</hibernate-mapping>
