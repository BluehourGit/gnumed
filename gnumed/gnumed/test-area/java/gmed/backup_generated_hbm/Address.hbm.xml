<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class 
		name="xgmed.domain.accountability.Address"
		table="address"
	>

	    <id 
			name="id"
			type="long"
		>
			<generator class="hilo.long">
			</generator>
	    </id>

        <property
	    	name="postcode"
            type="int"
            column="postcode"
            not-null="false"
			unique="false"
		/>

        <property
	    	name="state"
            type="java.lang.String"
            column="state"
            not-null="false"
			unique="false"
		/>

        <property
	    	name="street"
            type="java.lang.String"
            column="street"
            not-null="false"
			unique="false"
		/>

        <property
	    	name="suburb"
            type="java.lang.String"
            column="suburb"
            not-null="false"
			unique="false"
		/>

	    <!--
			To add non XDoclet properties, create a file named
			    hibernate-properties-Address.xml
			containing the additional properties and place it in your merge dir.
	    -->

	</class>

</hibernate-mapping>
