<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class 
		name="xgmed.domain.accountability.Telephone"
		table="telephone"
	>

	    <id 
			name="id"
			type="long"
		>
			<generator class="hilo.long">
			</generator>
	    </id>

        <property
	    	name="number"
            type="java.lang.String"
            column="number"
            not-null="false"
			unique="false"
		/>

		<bag role="partys" table="party_telephone_link" readonly="true" lazy="true">
		<key column="telephone_id"/>
		<many-to-many class="xgmed.domain.accountability.Party" column="party_id"/>
	</bag>

	</class>

</hibernate-mapping>
