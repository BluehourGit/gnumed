<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class 
		name="xgmed.domain.accountability.AccountabilityType"
		table="accountability_type"
	>

	    <id 
			name="id"
			type="long"
		>
			<generator class="hilo.long">
			</generator>
	    </id>

        <property
	    	name="description"
            type="java.lang.String"
            column="description"
            not-null="false"
			unique="false"
		/>

<set role="commissioners" table="accountability_type_commissioner_party_type" lazy="true">
<key column="accountability_type_id"/>
<many-to-many class="xgmed.domain.accountability.PartyType" column="party_type_id"/>
</set>

 <set role="responsibles" table="accountability_type_responsible_party_type" lazy="true">
     <key column="accountability_type_id"/>
     <many-to-many class="xgmed.domain.accountability.PartyType" column="party_type_id"/>
 </set>

	</class>

</hibernate-mapping>
