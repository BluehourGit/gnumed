<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.drugref.subsidized_products"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Integer"
        >
            <generator class="hilo">
            </generator>
        </id>

        <many-to-one
            name="product"
            class="org.drugref.product"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="id_product"
        />

        <many-to-one
            name="subsidies"
            class="org.drugref.subsidies"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="id_subsidy"
        />

        <property
            name="quantity"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="quantity"
        />

        <property
            name="max_rpt"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="max_rpt"
        />

        <property
            name="copayment"
            type="java.lang.Double"
            update="true"
            insert="true"
            column="copayment"
        />

        <property
            name="comment"
            type="java.lang.String"
            update="true"
            insert="true"
            column="comment"
        />

        <property
            name="restriction"
            type="java.lang.String"
            update="true"
            insert="true"
            column="restriction"
        />

        <property
            name="audit_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="audit_id"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-subsidized_products.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
