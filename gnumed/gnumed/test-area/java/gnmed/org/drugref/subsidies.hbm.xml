<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.drugref.subsidies"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Integer"
        >
            <generator class="hilo">
            </generator>
        </id>

        <property
            name="iso_countrycode"
            type="java.lang.String"
            update="true"
            insert="true"
            column="iso_countrycode"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="comment"
            type="java.lang.String"
            update="true"
            insert="true"
            column="comment"
        />

        <property
            name="audit_id"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="audit_id"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-subsidies.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
