<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.gnumed.gmIdentity.identity"
        proxy="org.gnumed.gmIdentity.identity"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="hilo">
            </generator>
        </id>

        <set
            name="social_identitys"
            lazy="false"
            inverse="true"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmIdentity.social_identity"
              />
        </set>

        <set
            name="identities_addressess"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmGIS.identities_addresses"
              />
        </set>

        <set
            name="clin_health_issues"
            lazy="false"
            inverse="true"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmClinical.clin_health_issue"
              />
        </set>

        <set
            name="clin_encounters"
            lazy="false"
            inverse="true"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmClinical.clin_encounter"
              />
        </set>

        <set
            name="namess"
            lazy="false"
            inverse="true"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmIdentity.Names"
              />
        </set>

        <set
            name="script_drugs"
            lazy="false"
            inverse="false"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmClinical.script_drug"
              />
        </set>

        <property
            name="pupic"
            type="java.lang.String"
            update="true"
            insert="true"
            column="pupic"
        />

        <property
            name="karyotype"
            type="java.lang.String"
            update="true"
            insert="true"
            column="karyotype"
        />

        <property
            name="dob"
            type="java.util.Date"
            update="true"
            insert="true"
            column="dob"
        />

        <property
            name="cob"
            type="java.lang.String"
            update="true"
            insert="true"
            column="cob"
        />

        <property
            name="deceased"
            type="java.util.Date"
            update="true"
            insert="true"
            column="deceased"
        />

        <many-to-one
            name="mobile"
            class="org.gnumed.gmGIS.telephone"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            column="mobile"
        />

        <set
            name="clin_attributes"
            lazy="false"
            inverse="false"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmClinical.clin_attribute"
              />
        </set>

        <set
            name="roles"
            lazy="false"
            inverse="false"
            cascade="all"
            sort="unsorted"
        >

              <key
                  column="identity"
              />

              <one-to-many
                  class="org.gnumed.gmIdentity.identity_role_info"
              />
        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-identity.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
